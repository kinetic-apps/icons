{
  "name": "@kineticapps/icons",
  "version": "0.0.0",
  "description": "Kinetic Apps icon library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./line": {
      "types": "./dist/line-1_5px.d.ts",
      "import": "./dist/line-1_5px.mjs",
      "require": "./dist/line-1_5px.js"
    },
    "./solid": {
      "types": "./dist/solid.d.ts",
      "import": "./dist/solid.mjs",
      "require": "./dist/solid.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "npm run clean && npm run build:icons && npm run build:native && npm run build:package",
    "build:icons": "node scripts/build-icons.js",
    "build:native": "node scripts/build-icons-native.js",
    "build:package": "tsup",
    "clean": "rm -rf dist src/components",
    "prepublishOnly": "npm run build",
    "release": "semantic-release"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kineticapps/icons.git"
  },
  "keywords": [
    "icons",
    "svg",
    "react",
    "react-native",
    "kinetic"
  ],
  "author": "Kinetic Apps",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kineticapps/icons/issues"
  },
  "homepage": "https://github.com/kineticapps/icons#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native-svg": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  },
  "devDependencies": {
    "@svgr/babel-plugin-add-jsx-attribute": "^8.0.0",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "camelcase": "^8.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "react": "^18.0.0",
    "semantic-release": "^22.0.0",
    "svgo": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}